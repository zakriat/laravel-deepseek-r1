{
    "name": "zakriat/laravel-deepseek-r1",
    "description": "Laravel-first Deepseek R1 AI integration with Guzzle",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/contracts": "^11.0",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Zakriat\\DeepseekR1\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zakriat\\DeepseekR1\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zakriat\\DeepseekR1\\DeepseekServiceProvider"
            ],
            "aliases": {
                "DeepseekR1": "Zakriat\\DeepseekR1\\Facades\\DeepseekR1"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Zakria Tunio",
            "email": "mzakriatunio@gmail.com",
            "role": "Developer"
        }
    ]
}