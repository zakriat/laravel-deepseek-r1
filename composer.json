{
    "name": "zakriat/laravel-deepseek-r1",
    "description": "Laravel-first Deepseek R1 AI integration with Guzzle",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": [
        "deepseek",
        "laravel",
        "api",
        "ai",
        "guzzle",
        "integration"
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "guzzlehttp/guzzle": "^7.8",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.6.7",
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Zakriat\\DeepseekR1\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zakriat\\DeepseekR1\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zakriat\\DeepseekR1\\DeepseekServiceProvider"
            ],
            "aliases": {
                "DeepseekR1": "Zakriat\\DeepseekR1\\Facades\\DeepseekR1"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "authors": [
        {
            "name": "Zakria Tunio",
            "email": "mzakriatunio@gmail.com",
            "role": "Developer",
            "homepage": "https://github.com/zakriat"
        }
    ],
    "homepage": "https://github.com/zakriat/laravel-deepseek-r1",
    "support": {
        "issues": "https://github.com/zakriat/laravel-deepseek-r1/issues",
        "source": "https://github.com/zakriat/laravel-deepseek-r1"
    }
}
